# Ignore node_modules folder
node_modules/

# Ignore environment variables file
.env

# Ignore logs and error output
logs/
*.log
npm-debug.log*

# Ignore OS-specific files
.DS_Store  # macOS specific
Thumbs.db  # Windows specific

# Ignore Prisma generated files
/prisma/migrations/
/prisma/dev.db
.prisma/

